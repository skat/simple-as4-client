                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="PreviousDocument" minOccurs="1" maxOccurs="unbounded" type="PreviousDocument">
            </xs:element>
            <xs:element name="UCR" minOccurs="0" maxOccurs="1" type="UCR">
