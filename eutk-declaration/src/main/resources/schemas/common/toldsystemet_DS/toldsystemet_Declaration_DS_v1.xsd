                    <xs:sequence>
                        <xs:element name="SequenceNumeric"
                                    type="ds:CommodityClassificationSequenceNumericType"
                                    minOccurs="1">
                            <xs:annotation>
                                <xs:documentation>
                                    <submission>
                                        Fill in sequential numbers.
                                    </submission>
                                    <amendment>
                                        Can not be corrected for an amendment.
                                    </amendment>
                                    <correction>
                                        Can be corrected for a correction.
                                    </correction>
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="ID"
                                    type="ds:ClassificationIdentificationIDType"
                                    minOccurs="1">
                            <xs:annotation>
                                <xs:documentation>
                                    <type>EUCDM 5.2</type>
                                    <code>6/14</code>
                                    <details>
                                        Type of goods.
                                        6 characters.
                                        For relevant commodity codes see:
                                        https://skat.dk/skat.aspx?oid=2247456
                                    </details>
                                    <submission>
                                        See details above.
                                    </submission>
                                    <amendment>
                                        Can be corrected for an amendment.
                                    </amendment>
                                    <correction>
                                        Can be corrected for a correction.
                                    </correction>
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="IdentificationTypeCode"
                                    type="ds:ClassificationIdentificationTypeCodeType"
                                    minOccurs="1">
                            <xs:annotation>
                                <xs:documentation>
                                    <type>DMS Specific field</type>
                                    <details>
                                        Set to TSP for all H7 declarations.
                                        Scheduled for future removal.
                                    </details>
                                    <submission>
                                        Fill in 'TSP' for submission.
                                    </submission>
                                    <amendment>
                                        Fill in 'TSP' for an amendment.
                                    </amendment>
                                    <correction>
                                        Fill in 'TSP' for a correction.
                                    </correction>
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="GoodsMeasure" minOccurs="1" maxOccurs="1" type="GoodsMeasure-H7">
                </xs:element>
